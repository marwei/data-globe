<%= form_for [@globe, Point.new], remote: true do |f| %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <%= f.label :country %>
  <%= f.text_field :country %>
  <%= f.label :magnitude %>
  <%= f.text_field :magnitude %>
  <%= f.submit "Create" %>
<% end %>